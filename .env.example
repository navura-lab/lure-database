# Lure Database - Environment Variables
# Copy this file to .env and fill in your values.

# --- R2 (Cloudflare) ---
R2_ENDPOINT=https://YOUR_ACCOUNT_ID.r2.cloudflarestorage.com
R2_BUCKET=lure-db-images
R2_PUBLIC_URL=https://pub-XXXX.r2.dev
R2_ACCESS_KEY_ID=your_r2_access_key
R2_SECRET_ACCESS_KEY=your_r2_secret_key
R2_REGION=auto

# --- Supabase ---
SUPABASE_URL=https://YOUR_PROJECT.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# --- Airtable ---
AIRTABLE_PAT=patXXXXXXXXXX.XXXXXXXX
AIRTABLE_BASE_ID=appXXXXXXXXXX
AIRTABLE_LURE_URL_TABLE_ID=tblXXXXXXXXXX
AIRTABLE_MAKER_TABLE_ID=tblXXXXXXXXXX

# --- Vercel ---
VERCEL_DEPLOY_HOOK=https://api.vercel.com/v1/integrations/deploy/prj_XXXX/XXXX
